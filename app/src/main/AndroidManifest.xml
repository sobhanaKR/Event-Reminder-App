<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.AlarmDate">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name="com.example.AlarmDate.data.ShoppingApplication"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.App.Starting">
    <activity android:name="com.example.AlarmDate.AlarmDateActivity"
        android:theme="@style/Theme.App.Starting"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
        <activity
            android:name=".CalendarEventActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.NotificationsGuide"/>
        <receiver android:name=".CounterNotificationReceiver"/>
    </application>

</manifest>